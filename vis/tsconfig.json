{
    "compilerOptions": {
        "target": "ES5",
        "module": "ESNEXT",
        "lib": ["dom", "ESNEXT"],
        "strict": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "jsx": "react-jsx",
        "noEmit": true,
        "isolatedModules": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true
    },
    "include": ["app/**/*"]
}